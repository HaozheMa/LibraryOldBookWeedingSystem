        <dd>
            @Html.DisplayFor(model => model.OffTime)
        </dd>

        <dt>
            最新离架时长换算
        </dt>

        <dd>
            @Html.DisplayFor(model => model.OffTimeCount)
        </dd>

        <dt>
            BWI
        </dt>

        <dd>
            @Html.DisplayFor(model => model.BWI)
        </dd>

    </dl>
