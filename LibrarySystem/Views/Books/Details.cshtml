@model LibrarySystem.Models.Books

@{
    ViewBag.Title = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Details</h2>

<div>
    <h4>Books</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            索书号
        </dt>

        <dd>
            @Html.DisplayFor(model => model.CallNum)
        </dd>

        <dt>
            作者
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Author)
        </dd>

        <dt>
           书名
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Name)
        </dd>

        <dt>
            ISBN
        </dt>

        <dd>
            @Html.DisplayFor(model => model.ISBN)
        </dd>

        <dt>
            复本量
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Duplicates)
        </dd>

        <dt>
            单书流同频率
        </dt>

        <dd>
            @Html.DisplayFor(model => model.CirculationFrequency)
